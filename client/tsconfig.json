{
  "include": ["./src", "./types"],
  "exclude": ["./node_modules"],
  "compilerOptions": {
    "module": "ESNext",
    "target": "ESNext",
    "strict": true,
    "baseUrl": "./",
    "paths": { "*": ["web_modules/.types/*"], "@app/*": ["src/*"], "@components": ["src/components/*"] },
    "moduleResolution": "Node",
  }
}
